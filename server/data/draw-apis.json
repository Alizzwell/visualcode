      "params": [{"name": "index", "description": "하이라이트 제거 위치", "optional": true}]
    }, {
      "name": "swap",
      "description": "Chart의 두 위치를 swap한다.",
      "params": [
        {"name": "index1", "description": "swap할 위치 1"},
        {"name": "index2", "description": "swap할 위치 2"}
      ]
    }, {
      "name": "clear",
      "description": "Chart 데이터를 제거한다.",
      "params": []
    }]
  },

